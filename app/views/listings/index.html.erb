<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    New Building or Property
    <ol class="breadcrumb">
      <li>
        <%= link_to "Home", root_path %></a>
      </li>
      <li>
        <strong><%= link_to "Portfolio", listings_path %></strong>
      </li>
      <li class="active">
        <strong><%= link_to "New Listing", new_listing_path %></strong>
      </li>
    </ol>
  </div>
  <div class="col-lg-2">
  </div>
</div>
<p id="notice"><%= notice %></p>

<h1>Listings</h1>

<table>
  <thead>
    <tr>
      <th>Building</th>
      <th>Unit no</th>
      <th>Floor plan</th>
      <th>Rent</th>
      <th>Num fbaths</th>
      <th>Num hbaths</th>
      <th>Advertising</th>
      <th>Move in special</th>
      <th>Commission</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.building %></td>
        <td><%= listing.unit_no %></td>
        <td><%= listing.floor_plan %></td>
        <td><%= listing.rent %></td>
        <td><%= listing.num_fbaths %></td>
        <td><%= listing.num_hbaths %></td>
        <td><%= listing.advertising %></td>
        <td><%= listing.move_in_special %></td>
        <td><%= listing.commission %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>
