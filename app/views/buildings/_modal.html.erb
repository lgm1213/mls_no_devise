<div class="modal inmodal" id="myModal" tabindex="-1" role="dialog" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content animated bounceInRight">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
        <i class="fa fa-laptop modal-icon"></i>
        <h4 class="modal-title"> </h4>
        <small class="font-bold"><%= @building.development_name %></small>
      </div>
      <div class="modal-body">
        <p>
          #<%= @building.folio %>Lorem Ipsum Stuff(development)
        </p>
      </div>
      <div class="modal-footer">
        <%= link_to "See Building Listings", building_listings_path(@building), class: 'btn btn-blue' %>
        <%= link_to "See More Info", building_path(@building), class: 'btn btn-primary'%>  
        <button type="button" class="btn btn-white" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>