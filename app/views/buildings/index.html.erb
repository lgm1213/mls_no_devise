<div class="row wrapper border-bottom white-bg page-heading">
  <div class="col-lg-10">
    New Building or Property
    <ol class="breadcrumb">
      <li>
        <%= link_to "Home", root_path %></a>
      </li>
      <li class="active">
        <strong><%= link_to "Portfolio", buildings_path %></strong>
      </li>
      <li>
        <%= link_to "New Building or Property", new_building_path %>
      </li>
    </ol>
  </div>
  <div class="col-lg-2">
  </div>
</div>
<p id="notice"><%= notice %></p>
<h1>Buildings</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>County</th>
      <th>Area</th>
      <th>City</th>
      <th>Folio</th>
      <th>Street</th>
      <th>Compasspoint</th>
      <th>Streetname</th>
      <th>State</th>
      <th>Zip</th>
      <th>Zip4</th>
      <th>Unit</th>
      <th>Legal</th>
      <th>Zoning</th>
      <th>Geographical</th>
      <th>Area</th>
      <th>Municipcode</th>
      <th>Township</th>
      <th>Section</th>
      <th>Subdivision</th>
      <th>Parcel</th>
      <th>Mapcoordinates</th>
      <th>Elementaryschool</th>
      <th>Middleschool</th>
      <th>Seniorhighschool</th>
      <th>Subdivisonname</th>
      <th>Developmentname</th>
      <th>Modelname</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @buildings.each do |building| %>
      <tr>
        <td><%= building.county %></td>
        <td><%= building.area %></td>
        <td><%= building.city %></td>
        <td><%= building.folio %></td>
        <td><%= building.street %></td>
        <td><%= building.compass_point %></td>
        <td><%= building.street_name %></td>
        <td><%= building.state %></td>
        <td><%= building.zip %></td>
        <td><%= building.zip4 %></td>
        <td><%= building.unit %></td>
        <td><%= building.legal %></td>
        <td><%= building.zoning %></td>
        <td><%= building.geographical %></td>
        <td><%= building.area %></td>
        <td><%= building.municip_code %></td>
        <td><%= building.township %></td>
        <td><%= building.section %></td>
        <td><%= building.subdivision %></td>
        <td><%= building.parcel %></td>
        <td><%= building.map_coordinates %></td>
        <td><%= building.elementary_school %></td>
        <td><%= building.middle_school %></td>
        <td><%= building.senior_high_school %></td>
        <td><%= building.subdivision_name %></td>
        <td><%= building.development_name %></td>
        <td><%= building.model_name_in_mls %></td>
        <td><%= building.user.username %></td>
        <td><%= link_to 'Show', building %></td>
        <td><%= link_to 'Edit', edit_building_path(building) %></td>
        <td><%= link_to 'Destroy', building, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

